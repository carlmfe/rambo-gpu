set(PACKAGE_VERSION "@PROJECT_VERSION@")
set(PACKAGE_VERSION_MAJOR "@PROJECT_VERSION_MAJOR@")
set(PACKAGE_VERSION_MINOR "@PROJECT_VERSION_MINOR@")

if(NOT PACKAGE_FIND_VERSION_RANGE)
    # version 1.0.0 and below
    set(PACKAGE_VERSION_COMPATIBLE TRUE)
    if(PACKAGE_FIND_VERSION VERSION_GREATER PACKAGE_VERSION)
        set(PACKAGE_VERSION_SUITABLE FALSE)
    endif()
else()
    if(PACKAGE_FIND_VERSION_RANGE_MIN STREQUAL "INCLUDE" AND PACKAGE_FIND_VERSION_MIN VERSION_GREATER PACKAGE_VERSION)
        set(PACKAGE_VERSION_COMPATIBLE FALSE)
    elseif(PACKAGE_FIND_VERSION_RANGE_MAX STREQUAL "INCLUDE" AND PACKAGE_FIND_VERSION_MAX VERSION_LESS PACKAGE_VERSION)
        set(PACKAGE_VERSION_COMPATIBLE FALSE)
    else()
        set(PACKAGE_VERSION_COMPATIBLE TRUE)
    endif()
endif()
